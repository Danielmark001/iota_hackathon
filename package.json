{
  "name": "intellilend",
  "version": "0.1.0",
  "description": "AI-Powered DeFi Lending Platform on IOTA",
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "deploy": "node scripts/deploy-contracts.js",
    "start:backend": "cd backend && node server.js",
    "start:ai": "cd ai-model/api && python app.py",
    "start:frontend": "cd frontend && npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:ai\" \"npm run start:frontend\"",
    "test": "npm run test:backend && npm run test:ai",
    "test:backend": "cd backend && npm test",
    "test:ai": "cd ai-model && python -m unittest discover tests",
    "lint": "eslint \"**/*.js\"",
    "bridge:test": "node scripts/test-cross-layer-bridge.js",
    "docs": "cd docs && mkdocs serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/intellilend.git"
  },
  "keywords": [
    "iota",
    "defi",
    "ai",
    "lending",
    "blockchain",
    "smart-contracts",
    "move",
    "evm"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/intellilend/issues"
  },
  "homepage": "https://github.com/yourusername/intellilend#readme",
  "dependencies": {
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "hardhat": "^2.23.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1"
  }
}
