{
  "name": "intellilend",
  "version": "1.0.0",
  "description": "AI-Powered DeFi Lending Platform on IOTA",
  "main": "backend/server.js",
  "scripts": {
    "test": "npx hardhat test",
    "server": "node backend/server.js",
    "frontend": "cd frontend && npm start",
    "demo": "node scripts/demo.js",
    "deploy": "npx hardhat run scripts/deploy.js --network iota-testnet",
    "start": "concurrently \"npm run server\" \"npm run frontend\"",
    "install-all": "npm install && cd frontend && npm install",
    "lint": "eslint . --ext .js,.jsx",
    "build": "cd frontend && npm run build",
    "ai-train": "cd ai-model && python train_model.py"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@openzeppelin/contracts": "^4.8.0",
    "axios": "^1.2.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "figlet": "^1.8.1",
    "inquirer": "^8.2.6",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "hardhat": "^2.12.6",
    "nodemon": "^2.0.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/iota-hackathon.git"
  },
  "keywords": [
    "iota",
    "defi",
    "ai",
    "lending",
    "blockchain",
    "move",
    "evm"
  ],
  "author": "Your Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/iota-hackathon/issues"
  },
  "homepage": "https://github.com/yourusername/iota-hackathon#readme"
}
